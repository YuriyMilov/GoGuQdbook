@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .

@prefix pops: <http://clarkparsia.com/pellet/tutorial/pops#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

@prefix : <http://clarkparsia.com/pellet/tutorial/pops-foaf-alignment#> .

pops:Employee rdfs:subClassOf foaf:Person .
pops:Project owl:equivalentClass foaf:Project .
pops:Organization owl:equivalentClass foaf:Organization .

pops:hasEmployee rdfs:subPropertyOf foaf:member .

pops:firstName owl:equivalentProperty foaf:givenname .
pops:lastName owl:equivalentProperty foaf:surname .
pops:mbox_sha1sum owl:equivalentProperty foaf:mbox_sha1sum .
