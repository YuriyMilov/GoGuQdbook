<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping> 
	<class name="com.gwtapps.databaseeditor.client.model.Story" table="stories"> 
		<id column="id" name="id" type="java.lang.String"> 
            <generator class="org.hibernate.id.UUIDHexGenerator"> 
            </generator> 
        </id> 
		<property name="title"/> 
		<property name="url"/> 
		<property name="description"/>
		<list name="digs" table="user_dug" cascade="save-update"> 
			<key column="story_id"/> 
			<list-index column="dig_id"/> 
            <many-to-many class="com.gwtapps.databaseeditor.client.model.User" column="user_id"/>
        </list> 
		<property name="user_id"/> 
	</class> 
</hibernate-mapping> 
